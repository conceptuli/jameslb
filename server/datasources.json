{
  "db": {
    "name": "db",
    "connector": "memory"
  },
  "tts": {
    "name": "tts",
    "connector": "rest",
    "operations": [
      {
        "template": {
          "method": "GET",
          "url": "http://localhost:3000/tts?phrase={phrase}",
          "headers": {
            "Content-type": "application/json"
          }

        },
        "functions": {
          "sayThis": [
            "phrase"
          ]
        }
      }
    ]
  }
}
